{
  "name": "redux-async-fetcher",
  "version": "0.0.0-development",
  "description": "Extract data fetching logic from your component lifecycle ",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "babel --out-dir dist --ignore *.test.js,./src/test/* src",
    "prebuild": "rimraf dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "semantic-release:pre": "semantic-release pre"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wing-eu/redux-async-fetcher.git"
  },
  "keywords": [
    "react",
    "redux",
    "data",
    "fetcher",
    "async",
    "hoc"
  ],
  "author": "Luc Merceron <luc.merceron.developer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wing-eu/redux-async-fetcher/issues"
  },
  "homepage": "https://github.com/wing-eu/redux-async-fetcher#readme",
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-redux": "7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babelify": "10.0.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "istanbul": "0.4.5",
    "jest": "24.8.0",
    "jest-babel-istanbul": "0.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.8.6",
    "redux": "4.0.1",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.18"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
