{
  "name": "redux-async-fetcher",
  "version": "0.0.0-development",
  "description": "Extract data fetching logic from your component lifecycle ",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "babel --out-dir dist --ignore *.test.js,./src/test/* src",
    "prebuild": "rimraf dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "semantic-release:pre": "semantic-release pre"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wing-eu/redux-async-fetcher.git"
  },
  "keywords": [
    "react",
    "redux",
    "data",
    "fetcher",
    "async",
    "hoc"
  ],
  "author": "Luc Merceron <luc.merceron.developer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wing-eu/redux-async-fetcher/issues"
  },
  "homepage": "https://github.com/wing-eu/redux-async-fetcher#readme",
  "dependencies": {
    "prop-types": "15.5.8",
    "react": "15.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "19.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babelify": "7.3.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.2.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jasmine": "2.7.1",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "istanbul": "0.4.5",
    "jest": "20.0.4",
    "jest-babel-istanbul": "0.1.0",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "redux": "3.7.2",
    "redux-mock-store": "1.2.3",
    "redux-thunk": "2.2.0",
    "rimraf": "2.6.1",
    "semantic-release": "^6.3.6"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ]
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
